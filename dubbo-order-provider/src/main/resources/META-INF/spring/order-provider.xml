<?xml version="1.0" encoding="UTF-8"?>
<!--suppress ALL -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd
         http://code.alibabatech.com/schema/dubbo
         http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--  名称     -->
    <dubbo:application name="order-provider" owner="husky"/>

    <!-- 注册中心  -->
    <!--<dubbo:registry address="N/A"/>-->

    <dubbo:registry protocol="zookeeper" address="139.199.32.78:2181"/>


    <!--  协议   -->
    <dubbo:protocol name="dubbo" port="20880"/>

    <dubbo:protocol name="hessian" port="8090" server="jetty"/>


    <!--  服务   -->
    <dubbo:service interface="com.uu.husky.IOrderService"
                   ref="orderService" protocol="dubbo,hessian"/>

    <!--Bean bean定义-->
    <bean id="orderService" class="com.uu.husky.OrderServiceImpl"/>

</beans>